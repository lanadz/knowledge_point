<table>
    <% @users.each do |user| %>
       <tr>
           <td>
                <%= user.id %>
           </td>

           <td>
                <%= user.first_name %>
           </td>

           <td>
                <%= user.last_name %>
           </td>

           <td>
                <%= user.is_mentor %>
           </td>
       </tr>
    <% end %>
</table>
